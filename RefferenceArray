
package arrays;
import java.util.*;
public class RefferenceArray {
    public static int[] Marks(int ar[])
    {
        for(int i=0;i<ar.length;i++)
        {
         ar [i] = ar[i]+1;
        }
        return ar;
    }
    
     public static void main(String[] args) {
         int ar[] = {97,98,99};
         int arr[]=Marks(ar);
         for(int i=0;i<arr.length;i++)
         {
             System.out.println(arr[i]);
         }
        
    }
    
}
